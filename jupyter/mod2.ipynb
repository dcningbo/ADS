    "def last_digit(n):\n",
    "    return n % 10\n",
    "\n",
    "def mod2(n):\n",
    "    return last_digit(n) % 2\n",
    "\n",
    "n = input(\"Enter number:\")\n",
    "n = int(n)\n",
    "result = mod2(n)\n",
    "\n",
    "print('{} in mod 2 is {}'.format(str(n), str(result)))"
   ]
